   <field name="nsrStockTrading" type="pdouble" indexed="true" stored="true" required="false" multiValued="false" default="0.0" /> 
   <field name="nsrDerivativeTrading" type="pdouble" indexed="true" stored="true" required="false" multiValued="false" default="0.0" /> 
   <field name="nsrDmargin" type="pdouble" indexed="true" stored="true" required="false" multiValued="false" default="0.0" /> 
   <field name="nsrDealFinancing" type="pdouble" indexed="true" stored="true" required="false" multiValued="false" default="0.0" />
